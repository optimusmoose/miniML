{
  "weka_executable_src": "C:/\"Program Files\"/Weka-3-6/weka.jar",
  "tolerance_ratio": 0.05,
  "runs_per_algorithm": 3,
  "jre_maximum_memory": 1024,
  "dataset_directory": "C:/\"Program Files\"/Weka-3-6/data",
  "attribute_selection_algorithms": [
    {
      "run_name": "general_attribute_selection",
      "datasets": [
        "soybean.arff",
        "iris.arff",
        "ionosphere.arff",
        "cpu.arff",
        "supermarket.arff"
